{
  "name": "rockbuzz/lara-tags",
  "description": "Tag management",
  "keywords": ["laravel", "tags", "categories"],
  "license": "MIT",
  "authors": [
    {
      "name": "Tiago Lopes",
      "email": "tiago@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraTags\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "spatie/laravel-sluggable": "~2.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "orchestra/testbench": "^4.0",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraTags\\ServiceProvider"
      ]
    }
  }
}
